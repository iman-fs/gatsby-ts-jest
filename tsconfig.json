{
  "include": ["./src/**/*", "./src/svg.d.ts"],
  "jsx": "react",
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noEmit": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "downlevelIteration": true
  }
}
